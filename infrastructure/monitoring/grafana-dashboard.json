{
  "dashboard": {
    "id": null,
    "title": "VoxLink Production Monitoring",
    "tags": ["voxlink", "production"],
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "panels": [
      {
        "id": 1,
        "title": "System Overview",
        "type": "row",
        "collapsed": false,
        "gridPos": {
          "h": 1,
          "w": 24,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "API Gateway Response Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "aws_applicationelb_target_response_time_average{load_balancer=\"voxlink-production-alb\"}",
            "legendFormat": "Response Time (avg)",
            "refId": "A"
          },
          {
            "expr": "aws_applicationelb_target_response_time_p95{load_balancer=\"voxlink-production-alb\"}",
            "legendFormat": "Response Time (p95)",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "min": 0,
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.5},
                {"color": "red", "value": 1.0}
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 1
        }
      },
      {
        "id": 3,
        "title": "Request Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(aws_applicationelb_request_count_sum{load_balancer=\"voxlink-production-alb\"}[5m]))",
            "legendFormat": "Requests/sec",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "min": 0
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 1
        }
      },
      {
        "id": 4,
        "title": "HTTP Status Codes",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(aws_applicationelb_httpcode_target_2xx_count_sum{load_balancer=\"voxlink-production-alb\"}[5m])",
            "legendFormat": "2XX Success",
            "refId": "A"
          },
          {
            "expr": "rate(aws_applicationelb_httpcode_target_4xx_count_sum{load_balancer=\"voxlink-production-alb\"}[5m])",
            "legendFormat": "4XX Client Error",
            "refId": "B"
          },
          {
            "expr": "rate(aws_applicationelb_httpcode_target_5xx_count_sum{load_balancer=\"voxlink-production-alb\"}[5m])",
            "legendFormat": "5XX Server Error",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "min": 0
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 9
        }
      },
      {
        "id": 5,
        "title": "Service Performance",
        "type": "row",
        "collapsed": false,
        "gridPos": {
          "h": 1,
          "w": 24,
          "x": 0,
          "y": 17
        }
      },
      {
        "id": 6,
        "title": "ECS Service CPU Utilization",
        "type": "timeseries",
        "targets": [
          {
            "expr": "aws_ecs_cpuutilization_average{service_name=\"api-gateway\"}",
            "legendFormat": "API Gateway",
            "refId": "A"
          },
          {
            "expr": "aws_ecs_cpuutilization_average{service_name=\"number-service\"}",
            "legendFormat": "Number Service",
            "refId": "B"
          },
          {
            "expr": "aws_ecs_cpuutilization_average{service_name=\"billing-service\"}",
            "legendFormat": "Billing Service",
            "refId": "C"
          },
          {
            "expr": "aws_ecs_cpuutilization_average{service_name=\"notification-service\"}",
            "legendFormat": "Notification Service",
            "refId": "D"
          },
          {
            "expr": "aws_ecs_cpuutilization_average{service_name=\"ai-agent-service\"}",
            "legendFormat": "AI Agent Service",
            "refId": "E"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 70},
                {"color": "red", "value": 90}
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 18
        }
      },
      {
        "id": 7,
        "title": "ECS Service Memory Utilization",
        "type": "timeseries",
        "targets": [
          {
            "expr": "aws_ecs_memoryutilization_average{service_name=\"api-gateway\"}",
            "legendFormat": "API Gateway",
            "refId": "A"
          },
          {
            "expr": "aws_ecs_memoryutilization_average{service_name=\"number-service\"}",
            "legendFormat": "Number Service",
            "refId": "B"
          },
          {
            "expr": "aws_ecs_memoryutilization_average{service_name=\"billing-service\"}",
            "legendFormat": "Billing Service",
            "refId": "C"
          },
          {
            "expr": "aws_ecs_memoryutilization_average{service_name=\"notification-service\"}",
            "legendFormat": "Notification Service",
            "refId": "D"
          },
          {
            "expr": "aws_ecs_memoryutilization_average{service_name=\"ai-agent-service\"}",
            "legendFormat": "AI Agent Service",
            "refId": "E"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 80},
                {"color": "red", "value": 95}
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 18
        }
      },
      {
        "id": 8,
        "title": "Service Health Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=~\"voxlink-.*\"}",
            "legendFormat": "{{job}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"options": {"0": {"text": "DOWN", "color": "red"}}, "type": "value"},
              {"options": {"1": {"text": "UP", "color": "green"}}, "type": "value"}
            ],
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "green", "value": 1}
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 26
        }
      },
      {
        "id": 9,
        "title": "Database & Cache",
        "type": "row",
        "collapsed": false,
        "gridPos": {
          "h": 1,
          "w": 24,
          "x": 0,
          "y": 34
        }
      },
      {
        "id": 10,
        "title": "RDS Database Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "aws_rds_cpuutilization_average{dbinstance_identifier=\"voxlink-production-db\"}",
            "legendFormat": "CPU Utilization",
            "refId": "A"
          },
          {
            "expr": "aws_rds_database_connections_average{dbinstance_identifier=\"voxlink-production-db\"}",
            "legendFormat": "Database Connections",
            "refId": "B"
          },
          {
            "expr": "aws_rds_read_latency_average{dbinstance_identifier=\"voxlink-production-db\"}",
            "legendFormat": "Read Latency",
            "refId": "C"
          },
          {
            "expr": "aws_rds_write_latency_average{dbinstance_identifier=\"voxlink-production-db\"}",
            "legendFormat": "Write Latency",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 35
        }
      },
      {
        "id": 11,
        "title": "Redis Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "aws_elasticache_cpuutilization_average{cache_cluster_id=\"voxlink-production-redis-001\"}",
            "legendFormat": "CPU Utilization",
            "refId": "A"
          },
          {
            "expr": "aws_elasticache_curr_connections_average{cache_cluster_id=\"voxlink-production-redis-001\"}",
            "legendFormat": "Current Connections",
            "refId": "B"
          },
          {
            "expr": "aws_elasticache_cache_hit_rate_average{cache_cluster_id=\"voxlink-production-redis-001\"}",
            "legendFormat": "Cache Hit Rate",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 35
        }
      },
      {
        "id": 12,
        "title": "Business Metrics",
        "type": "row",
        "collapsed": false,
        "gridPos": {
          "h": 1,
          "w": 24,
          "x": 0,
          "y": 43
        }
      },
      {
        "id": 13,
        "title": "Key Business Metrics",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(voxlink_numbers_purchased_total[1h]))",
            "legendFormat": "Numbers Purchased/Hour",
            "refId": "A"
          },
          {
            "expr": "sum(rate(voxlink_calls_processed_total[1h]))",
            "legendFormat": "Calls Processed/Hour",
            "refId": "B"
          },
          {
            "expr": "sum(voxlink_active_numbers_total)",
            "legendFormat": "Active Numbers",
            "refId": "C"
          },
          {
            "expr": "sum(voxlink_revenue_total)",
            "legendFormat": "Total Revenue ($)",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 2,
            "thresholds": {
              "steps": [
                {"color": "green", "value": null}
              ]
            }
          }
        },
        "gridPos": {
          "h": 6,
          "w": 24,
          "x": 0,
          "y": 44
        }
      },
      {
        "id": 14,
        "title": "Call Success Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(voxlink_calls_successful_total[5m]) / rate(voxlink_calls_total[5m]) * 100",
            "legendFormat": "Call Success Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 90},
                {"color": "green", "value": 95}
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 50
        }
      },
      {
        "id": 15,
        "title": "AI Agent Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(voxlink_ai_agent_requests_total[5m])",
            "legendFormat": "AI Requests/sec",
            "refId": "A"
          },
          {
            "expr": "rate(voxlink_ai_agent_errors_total[5m]) / rate(voxlink_ai_agent_requests_total[5m]) * 100",
            "legendFormat": "AI Error Rate (%)",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 50
        }
      },
      {
        "id": 16,
        "title": "Security & Alerts",
        "type": "row",
        "collapsed": false,
        "gridPos": {
          "h": 1,
          "w": 24,
          "x": 0,
          "y": 58
        }
      },
      {
        "id": 17,
        "title": "Failed Login Attempts",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(voxlink_failed_logins_total[5m])",
            "legendFormat": "Failed Logins/sec",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0,
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 10}
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 59
        }
      },
      {
        "id": 18,
        "title": "Active Alerts",
        "type": "stat",
        "targets": [
          {
            "expr": "ALERTS{alertstate=\"firing\"}",
            "legendFormat": "{{alertname}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"options": {"0": {"text": "No Active Alerts", "color": "green"}}, "type": "value"}
            ],
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 1}
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 59
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "refresh": "30s",
    "schemaVersion": 30,
    "version": 1
  }
}